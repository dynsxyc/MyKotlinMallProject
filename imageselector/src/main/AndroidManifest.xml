<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhongjiang.dyn.imageselector">

    <application
        android:allowBackup="true"
        android:supportsRtl="true">
        <!-- 图片选择器 start -->
        <activity
            android:name="com.zhongjiang.dyn.imageselector.ImageSelectorActivity"
            android:configChanges="orientation|screenSize|keyboardHidden|navigation"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name="com.zhongjiang.dyn.imageselector.PreviewActivity"
            android:configChanges="orientation|screenSize|keyboardHidden|navigation"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name="com.zhongjiang.dyn.imageselector.ClipImageActivity"
            android:configChanges="orientation|screenSize|keyboardHidden|navigation"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <!-- 图片选择器 end -->
    </application>

</manifest>
